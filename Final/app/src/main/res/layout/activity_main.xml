<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#6A6A6A"
    android:backgroundTint="@android:color/background_light"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/btn_empleado"
        android:layout_width="134dp"
        android:layout_height="131dp"
        android:layout_marginStart="48dp"
        android:scaleType="fitStart"
        android:src="@drawable/empresas"
        app:layout_constraintBottom_toBottomOf="@+id/btn_empresa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btn_empresa"
        app:layout_constraintTop_toTopOf="@+id/btn_empresa"
        app:layout_constraintVertical_bias="0.0"
        tools:src="@drawable/empresas"
        tools:srcCompat="@drawable/empresas" />

    <ImageButton
        android:id="@+id/btn_empresa"
        android:layout_width="135dp"
        android:layout_height="127dp"
        android:layout_marginTop="180dp"
        android:scaleType="fitStart"
        android:src="@drawable/empresa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.173"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/empresa"
        tools:srcCompat="@drawable/empresa" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Empresas"
        app:layout_constraintEnd_toEndOf="@+id/btn_empresa"
        app:layout_constraintStart_toStartOf="@+id/btn_empresa"
        app:layout_constraintTop_toBottomOf="@+id/btn_empresa" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Listar Empresas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btn_empleado"
        app:layout_constraintStart_toStartOf="@+id/btn_empleado"
        app:layout_constraintTop_toBottomOf="@+id/btn_empleado"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>